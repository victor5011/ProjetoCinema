@model HomeViewModel

<div class="row no-gutter">
    

    @foreach (var item in Model.FilmesDestaques)
    {
        <div class="col-xs-2 col-md-2">
            <a class="portfolio-box" asp-action="List" asp-controller="Filme">
                <img src="@item.ImagemUrl" alt="Filmes" width="450" height="220"/>
            </a>
        </div>
    }
</div>